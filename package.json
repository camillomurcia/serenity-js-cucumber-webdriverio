{
  "name": "serenity-js-cucumber-webdriverio",
  "version": "1.0.0",
  "description": "Example project automation with webdriverio and cucumber",
  "main": "index.js",
  "scripts": {
    "postinstall": "serenity-bdd update",
    "clean": "rimraf target",
    "test:execute": "wdio wdio.conf.ts",
    "test:report": "serenity-bdd run --feature ./features",
    "test": "failsafe clean test:execute test:report",
    "lint": "eslint --ext ts --config .eslintrc.js .",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Juan Camilo Murcia Ramos",
  "license": "Apache-2.0",
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "@serenity-js/assertions": "^2.32.7",
    "@serenity-js/console-reporter": "^2.32.7",
    "@serenity-js/core": "^2.32.7",
    "@serenity-js/cucumber": "^2.32.7",
    "@serenity-js/rest": "^2.32.7",
    "@serenity-js/serenity-bdd": "^2.32.7",
    "@serenity-js/webdriverio": "^2.32.7",
    "@wdio/cli": "^7.16.13",
    "@wdio/cucumber-framework": "^7.16.13",
    "@wdio/local-runner": "^7.16.13",
    "@wdio/spec-reporter": "^7.16.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chromedriver": "^96.0.0",
    "expect-webdriverio": "^3.1.4",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "is-ci": "^3.0.1",
    "npm-failsafe": "^0.4.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "wdio-chromedriver-service": "^7.2.6"
  }
}
